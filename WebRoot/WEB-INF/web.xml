<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>test</servlet-name>
    <servlet-class>com.gesac.servlet.android.test</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getEmployee</servlet-name>
    <servlet-class>com.gesac.servlet.android.getEmployee</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>addMission</servlet-name>
    <servlet-class>com.gesac.servlet.android.addMission</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getMission</servlet-name>
    <servlet-class>com.gesac.servlet.android.getMission</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getEmployee</servlet-name>
        <url-pattern>/getEmployee.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addMission</servlet-name>
    <url-pattern>/addMission.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getMission</servlet-name>
        <url-pattern>/getMission.do</url-pattern>
  </servlet-mapping>

</web-app>